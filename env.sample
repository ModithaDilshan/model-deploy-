# Worker configuration template
# Copy this file to `.env` on the worker machine and replace the placeholder values.

# AWS configuration
AWS_REGION=us-east-1
UPLOADS_BUCKET=unity-builder-uploads
BUILDS_BUCKET=unity-builder-builds
JOBS_TABLE_NAME=unity_build_jobs
JOBS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/471624149802/unity-build-jobs
UPLOAD_URL_TTL=900
DOWNLOAD_URL_TTL=900

# Path to the prebuilt executable you want to distribute.
# Change this to the actual location on the worker machine.
PREBUILT_EXECUTABLE=D:/PrebuiltGame/MyGame.exe

# Unity build settings (only required if you run Unity builds instead of prebuilt executable)
UNITY_PROJECT_PATH=D:/New folder (40)/My project (1)
UNITY_EDITOR_PATH="C:/Program Files/Unity/Hub/Editor/2022.3.0f1/Editor/Unity.exe"
MODEL_TARGET_BASE=Assets/Resources/Character/Character
BUILD_OUTPUT_PATH=Builds/MyGame.exe
BUILD_METHOD=BuildScript.BuildGame

# Worker behavior
WORKER_POLL_INTERVAL=5000
WORKER_MAX_CONCURRENT_JOBS=1

