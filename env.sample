# gg.play Configuration Template
# Copy this file to `.env` and replace the placeholder values.

# AWS Configuration
AWS_REGION=us-east-1
UPLOADS_BUCKET=unity-builder-uploads
BUILDS_BUCKET=unity-builder-builds
JOBS_TABLE_NAME=unity_build_jobs
JOBS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/471624149802/unity-build-jobs
UPLOAD_URL_TTL=900
DOWNLOAD_URL_TTL=900

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/ggplay
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/ggplay

# Authentication Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Godot Build Settings (required for build worker)
GODOT_PROJECT_PATH=D:/New folder (40)/new-game-project
GODOT_EDITOR_PATH=C:/Program Files/Godot/Godot_v4.3-stable_win64.exe
GODOT_IMPORTED_MODEL_PATH=Imported/user_model.glb
GODOT_EXPORT_PRESET_WIN=Windows Desktop
GODOT_EXPORT_PRESET_WEB=Web
GODOT_WINDOWS_OUTPUT_NAME=MyGame.exe
GODOT_WEB_OUTPUT_NAME=index.html
SUPPORTED_MODEL_FORMATS=glb,obj

# Worker Behavior
WORKER_POLL_INTERVAL=5000
WORKER_MAX_CONCURRENT_JOBS=1

# Server Settings
PORT=3000

